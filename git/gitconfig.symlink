[include]
  path = ~/.dotprivate/gitconfig

[color]
  branch = auto
  diff = auto
  status = auto

[core]
  editor = vim

[diff]
  compactionHeuristic = true
  rename = copy

[pager]
  color = true

[push]
  default = simple

[filter "lfs"]
  clean = git-lfs clean %f
  smudge = git-lfs smudge %f
  required = true
